lexer ArithLexer -> {ArithToken.t} where

rules

start tok
    = '+' { ArithToken.Plus o #1 }
    | '*' { ArithToken.Times o #1 }
    | '(' { ArithToken.LParen o #1 }
    | ')' { ArithToken.RParen o #1 }
    | digits { fn (s, cs, e) => ArithToken.Num (s, valOf (Int.fromString cs), e) }
    ;

digits = [[:digit:]] (digits | );

whitespace ws = [[:space:]] ws | ;

